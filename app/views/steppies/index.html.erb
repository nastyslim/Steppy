        <div id="catall" class="row" style="padding-left: 7px;padding-right: 15px;text-align: center;overflow: hidden;padding-top:15px;">
            <% @steppy = Steppy.order('created_at DESC')%>

            <% @steppy.each do |steppy| %>
                
                    <%= link_to steppy do%>
                        <% if steppy.goal.present? %>
                            <div class="steppybx2">       
                                <div style="padding-left: 14px;">
                                    <div style="font-size: 12px;padding-top: 9px;color: grey;margin-bottom:-3px;">How to become  <%= steppy.goal.indefinite_article %>  </div>
                                    <div style="font-size:25px;"><b><%= truncate(steppy.goal.capitalize, :length => 40) %></b></div>
                                    <div><%= truncate(steppy.description.capitalize, :length => 150).html_safe %></div>
                                </div>
                            </div>
                        <% elsif steppy.ask.present? %>
                            <div class="requestbox steppybx2_ask" style="overflow: hidden;">       
                                <div style="padding-left:14px;float: left;">
                                    <div style="font-size: 12px;padding-top: 9px;color: white;margin-bottom:-3px;">How to become  <%= steppy.ask.indefinite_article %>  </div>
                                    <div style="font-size:25px;"><b><%= truncate(steppy.ask.capitalize, :length => 40) %></b></div>
                                </div>
                                <div style="padding-top: 20px;float: right;padding-right: 20px;">
                                    <p class="btn btn-primary" href="#" role="button">Answer!</p>
                                </div>
                            </div>
                        <% end %>
                    <% end %>
                        <div class="inlist" style="margin-top: -5px;padding-bottom: 10px;">
                            <ul style="padding-left:0px;">
                                <li>
                                    <% if current_user %>
                                        <% if steppy.liked_by?(current_user) %>
                                            <%= link_to "Unlike", dislike_steppy_path(steppy), method: :put, :class => "actionbtns_on" %>                                        
                                        <%else%>
                                            <%= link_to "Like", like_steppy_path(steppy), method: :put, :class => "actionbtns"  %>
                                        <%end%>
                                    <% else %>
                                        <%= link_to "Like", new_user_path, :class => "actionbtns" %>
                                    <%end%>
                                </li>  
                                <li><% if steppy.goal.present? %>
                                        <% if current_user %>
                                            <% if current_user.following?(steppy)%>
                                                <%= link_to 'Stop Climbing', unfollow_path(steppy), :class => "actionbtns_on"%>
                                            <%else%>
                                                <%= link_to 'Start Climbing', follow_path(steppy), :class => "actionbtns" %>
                                            <%end%>
                                        <% else %>
                                            <%= link_to "Start Climbing!", new_user_path, :class => "actionbtns" %>
                                        <% end %>
                                    <% end %>
                                </li>
                                <li>
                                    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
                                    <script src="social-likes.min.js"></script>

                                    <div class="social-likes social-likes_single" data-url="http://steppy.herokuapp.com/steppies/<%= steppy.id %>" data-title="Steppy - How to become a/an <%= steppy.goal %>" data-single-title="Share">
